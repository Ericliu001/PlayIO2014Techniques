<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eric.playio2014techniques" >


    <uses-sdk
        android:minSdkVersion="15"
        android:targetSdkVersion="L" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.HomeActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.schedule.MyScheduleActivity"
            android:label="@string/title_activity_my_schedule" >
        </activity>

        <activity android:name="com.eric.playio2014techniques.ui.sessions.BrowseSessionsActivity"
            android:label="@string/title_activity_browse_sessions"
            >
        </activity>
    </application>

</manifest>

